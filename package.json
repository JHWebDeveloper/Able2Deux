{
	"name": "able2",
	"version": "2.1.0",
	"description": "Able2 is an all-in-one News Editor's toolkit. Able2 is able to download video from the internet, do screen records and convert and process video for air.",
	"main": "build/main.js",
	"scripts": {
		"lint": "eslint . --ext .js",
		"dev": "webpack-dev-server --config webpack.dev.js",
		"build": "rimraf build && webpack --config webpack.prod.js",
		"build:devtools": "rimraf build && webpack --config webpack.prod.js --define process.end.devtools",
		"dist:mac": "rimraf dist && npm run build && electron-builder -m",
		"dist:win": "rimraf dist && npm run build && electron-builder -w",
		"dist:mac:devtools": "rimraf dist && npm run build:devtools && electron-builder -m",
		"dist:win:devtools": "rimraf dist && npm run build:devtools && electron-builder -w",
		"deploy:mac": "rimraf dist && npm run build && electron-builder build -m --publish always",
		"deploy:win": "rimraf dist && npm run build && electron-builder build -w --publish always",
		"postinstall": "electron-builder install-app-deps",
		"start": "electron .",
		"update-ytdl:mac": "node_modules/youtube-dl-ffmpeg-ffprobe-static/bin/darwin/x64/youtube-dl -U",
		"update-ytdl:win": "node_modules/youtube-dl-ffmpeg-ffprobe-static/bin/win32/x64/youtube-dl.exe -U"
	},
	"author": "Jonathan Hamilton",
	"license": "CC-BY-NC-4.0",
	"dependencies": {
		"axios": "^0.19.2",
		"electron-log": "^4.2.2",
		"electron-updater": "^4.3.1",
		"electron-util": "^0.14.2",
		"ffmpeg-static-electron": "^2.0.1",
		"ffprobe-static-electron": "^2.0.0",
		"fluent-ffmpeg": "^2.1.2",
		"get-pixels": "^3.3.2",
		"get-rgba-palette": "^2.0.1",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-router-dom": "^5.2.0",
		"toastr": "^2.1.4",
		"ts-ebml": "^2.0.2",
		"uuid": "^8.2.0",
		"youtube-dl-ffmpeg-ffprobe-static": "^1.2.1"
	},
	"devDependencies": {
		"@babel/core": "^7.10.4",
		"@babel/plugin-proposal-class-properties": "^7.10.4",
		"@babel/preset-env": "^7.10.4",
		"@babel/preset-react": "^7.10.4",
		"@babel/register": "^7.10.4",
		"babel-loader": "^8.1.0",
		"babel-preset-minify": "^0.5.1",
		"copy-webpack-plugin": "^6.0.3",
		"core-js": "^3.6.5",
		"css-loader": "^3.6.0",
		"cssnano": "^4.1.10",
		"electron": "^9.1.0",
		"electron-builder": "^22.7.0",
		"eslint": "^7.4.0",
		"eslint-plugin-react": "^7.20.3",
		"file-loader": "^6.0.0",
		"html-webpack-plugin": "^4.3.0",
		"mini-css-extract-plugin": "^0.9.0",
		"nodemon": "^2.0.4",
		"postcss": "^7.0.32",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"regenerator-runtime": "^0.13.5",
		"rimraf": "^3.0.2",
		"url-loader": "^4.1.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-node-externals": "^2.3.0"
	},
	"browserslist": "electron 6.0.1"
}
