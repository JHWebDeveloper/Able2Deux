	"main": "build/main.js",
	"scripts": {
		"lint": "eslint . --ext .js",
		"dev": "webpack-dev-server --config webpack.dev.js",
		"build": "rimraf build && webpack --config webpack.prod.js",
		"pack": "rimraf dist && electron-builder --dir",
		"dist": "rimraf dist && npm run build && electron-builder -w -m",
		"deploy": "rimraf dist && npm run build && electron-builder build -w -m --publish always",
		"postinstall": "electron-builder install-app-deps",
		"start": "electron .",
		"update-ytdl:mac": "node_modules/youtube-dl-ffmpeg-ffprobe-static/bin/darwin/x64/youtube-dl -U",
		"update-ytdl:win": "cd node_modules/youtube-dl-ffmpeg-ffprobe-static/bin/win32/ia32 && youtube-dl.exe -U && cd ../x64 && youtube-dl.exe -U"
	},
